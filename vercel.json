{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { 
        "src": "/api/backend/(.*)",
        "dest": "backend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }
  